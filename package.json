    "mysql2": "^3.9.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "pkg": "^5.8.1",
    "cpy-cli": "^5.0.0",
    "rimraf": "^5.0.7",
    "mkdirp": "^3.0.1",
